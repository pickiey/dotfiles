# linuxbrew
BREW_HOME=$HOME/.linux
export PATH="$BREW_HOME/bin:$PATH"
export MANPATH="$BREW_HOME/share/man:$MANPATH"
export INFOPATH="$BREW_HOME/share/info:$INFOPATH"
export LD_LIBRARY_PATH="$BREW_HOME/lib:$LD_LIBRARY_PATH"
