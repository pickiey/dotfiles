# linuxbrew
BREWHOME=$HOME/.linux
export PATH="$BREWHOME/bin:$PATH"
export MANPATH="$BREWHOME/share/man:$MANPATH"
export INFOPATH="$BREWHOME/share/info:$INFOPATH"
export LD_LIBRARY_PATH="$BREWHOME/lib:$LD_LIBRARY_PATH"
